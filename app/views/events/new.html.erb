<h1>Create Event</h1>
<%= form_for @event do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :venue %>
  <%= f.text_area :venue %>
  <%= f.label :time_start %>
  <%= f.datetime_field :time_start %>
  <%= f.label :time_end %>
  <%= f.datetime_field :time_end %>
  <%= f.submit "Create" %>
<% end %>
